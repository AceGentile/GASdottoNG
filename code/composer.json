{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aws/aws-sdk-php": "^3.38",
        "barryvdh/laravel-debugbar": "^2.4",
        "caffeinated/themes": "^3.0",
        "devfactory/minify": "^1.0",
        "digitick/sepa-xml": "^1.2",
        "doctrine/dbal": "^2.5",
        "elibyy/tcpdf-laravel": "^5.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "madbob/automail": "dev-master",
        "madbob/laravel-log2rss": "^0.1.0",
        "sinergi/browser-detector": "^6.1",
        "vespakoen/menu": "^3.0",
        "wilgucki/csv": "^0.6.2",
        "zetacomponents/archive": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Setup.php",
            "app/Helpers/Formatters.php",
            "app/Helpers/Reflection.php",
            "app/Helpers/Paths.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.4"
        }
    }
}
